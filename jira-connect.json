{
    "key": "APIDP-Jira-Webhook",
    "name": "Versa Networks Jira Webhook Connector",
    "description": "Webhooks for Jira",
    "vendor": {
        "name": "Versa-API-DP",
        "url": "https://versa-networks.com/"
    },
    "baseUrl": "https://bm1-us.apidp.versa-test.net/v1/jira",
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/webhook"
    },
    "scopes": [
        "READ"
    ],
    "modules": {
        "webhooks": [
        {
            "event": "jira:issue_created",
            "url": "/webhook"
        },
      	{
      	    "event": "jira:issue_updated",
      	    "url": "/webhook"
      	},
        {
      	    "event": "jira:issue_deleted",
      	    "url": "/webhook"
      	},
        {
            "event": "comment_created",
            "url": "/webhook"
        },
      	{
      	    "event": "comment_updated",
      	    "url": "/webhook"
      	},
        {
      	    "event": "comment_deleted",
      	    "url": "/webhook"
      	},
        {
            "event": "project_deleted",
            "url": "/webhook"
        },
      	{
      	    "event": "project_soft_deleted",
      	    "url": "/webhook"
      	},
        {
      	    "event": "project_archived",
      	    "url": "/webhook"
      	}
        ]
    }
}
